{
  "name": "Mavenlink Location Selector",
  "version": "0.1",
  "description": "Because you don't have time when you're logging time!",
  "manifest_version": 2,
  "permissions": ["https://app.mavenlink.com/timesheets*", "storage"],
  "content_scripts": [
    {
      "matches": ["https://app.mavenlink.com/timesheets*"],
      "js": ["content.js"],
      "run_at": "document_start"
    }
  ],
  "browser_action": {
    "default_title": "Mavenlink Location Selector",
    "default_popup": "menu.html"
  }
}
